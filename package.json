{
  "name": "pass-ui",
  "version": "0.3.0",
  "private": true,
  "description": "The user interface for the eclipse-pass project",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js:fix": "eslint . --fix",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --cache",
    "start": "ember server",
    "test": "concurrently \"npm:lint\" \"npm:test:no-serve\" --names \"lint,test:\"",
    "test:no-serve": "PASS_API_NAMESPACE=/mirage/test ember test",
    "test:serve": "PASS_API_NAMESPACE=/mirage/test ember test -s",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
