    "jquery": "^3.6.4",
    "lint-staged": "^13.2.0",
    "lint-to-the-future": "^2.0.0",
    "lint-to-the-future-ember-template": "^1.1.1",
    "lint-to-the-future-eslint": "^2.0.1",
    "loader.js": "^4.7.0",
    "merge": ">=1.2.1",
    "popper.js": "^1.14.5",
    "prettier": "^2.8.7",
    "qunit": "^2.19.2",
    "qunit-dom": "^2.0.0",
    "sinon": "^15.0.2",
    "string-hash": "^1.1.3",
    "sweetalert2": "^7.29.0",
    "webpack": "^5.76.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {}
